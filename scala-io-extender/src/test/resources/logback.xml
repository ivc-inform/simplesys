<?xml version="1.0" encoding="UTF-8"?>

<configuration>

  <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{255} - %msg%n</pattern>
    </encoder>
  </appender>

  <!--<logger name="com.simplesys.json.JsonObject" level="error"/>-->

  <root>
    <appender-ref ref="stdout"/>
  </root>
</configuration>
